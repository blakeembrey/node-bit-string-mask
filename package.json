  "bugs": {
    "url": "https://github.com/blakeembrey/node-bit-string-mask/issues"
  },
  "homepage": "https://github.com/blakeembrey/node-bit-string-mask",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.13",
    "jest": "^19.0.0",
    "rimraf": "^2.5.4",
    "tslint": "^5.0.0",
    "tslint-config-standard": "^5.0.2",
    "typescript": "^2.2.2"
  }
